<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="ImgWithText">
    <attr format="reference" name="img"/>
    <attr format="reference" name="text"/>
    <attr format="color" name="textColor"></attr>
  </declare-styleable>

  <declare-styleable name="LinearListView">
    <attr name="android:entries"/>
    <attr format="dimension" name="dividerThickness"/>
  </declare-styleable>

  <declare-styleable name="TextInputLayout">
    <attr format="reference" name="hint"/>
    <attr format="dimension" name="android:textSize"/>
    <attr format="color" name="hintColor"></attr>
    <attr name="editable" format="boolean"/>
  </declare-styleable>
</resources>
