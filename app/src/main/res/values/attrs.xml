<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="ImgWithText">
    <attr format="reference" name="img"/>
    <attr format="reference" name="text"/>
    <attr format="color" name="textColor"></attr>
  </declare-styleable>

  <declare-styleable name="LinearListView">
    <attr name="android:entries"/>
    <attr format="dimension" name="dividerThickness"/>
  </declare-styleable>

  <declare-styleable name="TextInputView">
    <attr format="string" name="hintText"/>
    <attr format="dimension" name="strSize"/>
    <attr format="color" name="editColor"></attr>
    <attr format="color" name="hintTextColor"></attr>
    <attr format="boolean" name="editTextable"/>
  </declare-styleable>
</resources>
